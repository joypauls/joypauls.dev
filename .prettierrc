{
  "arrowParens": "avoid",
  "semi": true,
  "tabWidth": 2,
  "printWidth": 80,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "**/*.svg",
      "options": { "parser": "markdown" }
    },
    {
      "files": "**/*.jpg",
      "options": { "parser": "markdown" }
    }
  ]
}
